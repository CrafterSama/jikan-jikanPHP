{
    "name": "jikan/jikan-php",
    "type": "library",
    "licence": "MIT",
    "require": {
        "guzzlehttp/guzzle": "~6",
        "jms/serializer": "^1.13",
        "symfony/yaml": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Jikan\\JikanPHP\\": [
                "src"
            ],
            "Jikan\\Model\\": [
                "model"
            ],
            "Jikan\\Request\\": [
                "request"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JikanPHP\\Test\\": [
                "test"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.3",
        "symfony/console": "^4.2",
        "symfony/var-dumper": "^4.2"
    }
}
